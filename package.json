{
  "name": "egangotri-node-backend",
  "version": "1.0.0",
  "description": "eGangotri Upload Monitoring and Reporting Tool",
  "main": "index.ts",
  "scripts": {
    "test": "npm test",
    "flatfile-to-mongo": "ts-node ./src/tasks/flatfile-to-mongo.ts",
    "start": "ts-node ./src/index.ts",
    "web-scrap": "ts-node ./src/downloadLinks.ts",
    "exec": "ts-node ./src/exec/exec.ts",
    "tif2pdfOrig": "ts-node ./src/imgToPdf/convert.ts",
    "tif2pdf": "node -r ts-node/register ./src/imgToPdf/convert.ts",
    "convert": "node -r ts-node/register --max-old-space-size=2048 --inspect --expose-gc ./src/imgToPdf/convert.ts",
    "tally": "node -r ts-node/register --max-old-space-size=2048 --inspect --expose-gc ./src/imgToPdf/tally.ts",
    "merge": "node -r ts-node/register --max-old-space-size=2048 --inspect --expose-gc ./src/imgToPdf/merge.ts",
    "png2pdf": "node -r ts-node/register --max-old-space-size=2048 --inspect --expose-gc ./src/imgToPdf/pngToPdf.ts",
    "shrinkpdf": "node -r ts-node/register  ./src/exec/pdfShrinkViaExec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eGangotri/egangotri-node-backend.git"
  },
  "keywords": [
    "eGangotri"
  ],
  "author": "eGangotri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eGangotri/egangotri-node-backend/issues"
  },
  "homepage": "https://github.com/eGangotri/egangotri-node-backend#readme",
  "dependencies": {
    "@types/node": "^16.11.12",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "csv-parser": "^3.0.0",
    "easy-pdf-merge": "^0.2.6",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^5.11.7",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^2.10.377",
    "pdfkit": "^0.13.0",
    "puppeteer": "^13.0.0",
    "sharp": "^0.29.3",
    "typescript": "^4.5.3",
    "underscore": "^1.13.1",
    "uuid": "^8.3.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "ts-node": "^10.4.0"
  }
}
