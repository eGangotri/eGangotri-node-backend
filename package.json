{
  "name": "egangotri-node-backend",
  "version": "1.0.0",
  "description": "eGangotri Upload Monitoring and Reporting Tool",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env  ts-node ./src/index.ts",
    "build": "npx tsc",
    "start": "cross-env  pnpm run build && node ./dist/index.js",
    "test": "pnpm test",
    "upload_db": "cross-env NODE_OPTIONS=--max-old-space-size=8192 ts-node ./src/index.ts forUpload",
    "web-scrap": "ts-node ./src/downloadLinks.ts",
    "flatfile-to-mongo": "ts-node ./src/tasks/flatfile-to-mongo.ts \"C:/Users/X/eGangotri\"",
    "validateListOfArchiveUrlsAsValid": "ts-node ./src/validateListOfArchiveUrlsAsValid/index.ts",
    "exec": "ts-node ./src/exec/exec.ts",
    "png2pdf": "node -r ts-node/register --max-old-space-size=4096 --inspect --expose-gc ./src/imgToPdf/pngToPdf.ts",
    "alpha": "node -r ts-node/register ./src/imgToPdf/utils/PngUtils.ts",
    "convert": "node -r ts-node/register --max-old-space-size=4096 --inspect --expose-gc ./src/imgToPdf/conversionService/convertTifToPDF.ts",
    "tally-post-conversion": "node -r ts-node/register ./src/imgToPdf/conversionService/postConversionTally.ts",
    "move-merged-pdfs": "node -r ts-node/register ./src/imgToPdf/conversionService/moveMergedPdfs.ts",
    "vanity": "ts-node ./src/vanityService/VanityPdf.ts",
    "catalog": "ts-node ./src/cliBased/googleapi/GoogleDriveApiReadAndExport.ts",
    "refresh": "ts-node ./src/cliBased/googleapi/_utils/getRefreshToken.ts",
    "extractFirstAndLastNPages": "cross-env NODE_OPTIONS=--max-old-space-size=16384 ts-node ./src/cliBased/pdf/extractFirstAndLastNPages.ts",
    "combineExcels": "ts-node ./src/cliBased/googleapi/_utils/CombineMainAndReducedExcelData",
    "excelutil": "ts-node ./src/cliBased/googleapi/_utils/ExcelUtils",
    "findLongestFileName": "ts-node ./src/utils/utils",
    "callUrl": "ts-node ./src/cliBased/googleapi/_utils/ExcelUtils",
    "moveLongNames": "ts-node ./src/cliBased/googleapi/_utils/MoveLongFileNamesToRoot",
    "tallyWithLocal": "ts-node ./src/cliBased/googleapi/_utils/TallyLocalFolderWithGoogleDriveExcel",
    "localToExcel": "ts-node ./src/cliBased/googleapi/_utils/LocalFolderToExcel",
    "syncFolders": "ts-node ./src/folderCompare",
    "compareExcels": "ts-node ./src/cliBased/excel/CompareExcels",
    "findNonLatin": "ts-node ./src/cliBased/excel/FindNonLatinCharactersInName",
    "downloadPdf": "ts-node ./src/cliBased/pdf/downloadPdf.ts",
    "downloadFromGoogle": "ts-node ./src/cliBased/googleapi/GoogleDriveApiReadAndDownload",
    "searchArchive": "ts-node ./src/cliBased/search/searchArchive",
    "profileFolder": "ts-node ./src/cliBased/utils",
    "scraper": "ts-node ./src/archiveDotOrg/archiveScraper",
    "addFooter": "ts-node ./src/footerToPdf/index2",
    "moveFilesToProfile": "ts-node ./src/cliBased/fileMover/index",
    "excelToMongoArchive": "ts-node ./src/excelToMongo/transferArchiveExcelToMongo",
    "excelToMongoGDrive": "ts-node ./src/excelToMongo/tranferGDriveExcelToMongo",
    "aksharamukha": "ts-node ./src/aksharamukha/convert",
    "extractFIPDataFromExcel": "ts-node ./src/_adHoc/fip/extractFIPDataFromExcel",
    "combineFIPExcels": "ts-node ./src/_adHoc/fip/combineFIPExcels",
    "fipRename": "ts-node ./src/_adHoc/fip/fipRename",
    "uploadFileToGDrive": "ts-node ./src/cliBased/googleapi/GoogleDriveUpload",
    "renameNonAsciiFiles": "ts-node ./src/files/renameNonAsciiFiles.ts",
    "jpgToPdf": "ts-node ./src/imgToPdf/jpgToPdf.ts",
    "convertEAPExcel": "ts-node ./src/eap_bl/index.ts",
    "zipFiles": "ts-node ./src/services/zipService.ts",
    "upload-to-archive": "ts-node ./src/uploadToArchive/index.ts",
    "extract-catalog": "ts-node ./src/cliBased/pdf/extractCatalogFromPDF.ts",
    "process-asha-catalog": "ts-node ./src/_adHoc/processAshaArchivesCatalog.ts",
    "copy-asha-pdfs": "ts-node ./src/_adHoc/ashaCatalogPdfCopy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eGangotri/egangotri-node-backend.git"
  },
  "keywords": [
    "eGangotri"
  ],
  "author": "eGangotri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eGangotri/egangotri-node-backend/issues"
  },
  "homepage": "https://github.com/eGangotri/egangotri-node-backend#readme",
  "dependencies": {
    "@csv-js/csv": "0.0.2",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "child_process": "^1.0.2",
    "csv-parse": "latest",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "docx-parser": "^0.2.1",
    "dotenv": "^16.4.7",
    "easy-pdf-merge": "^0.2.6",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "fs-extra": "^11.3.0",
    "g": "^2.0.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "mongodb": "^6.13.0",
    "mongoose": "^7.8.6",
    "node-downloader-helper": "^2.1.9",
    "openai": "^4.85.2",
    "opossum": "^8.4.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "pdfkit": "^0.13.0",
    "puppeteer": "^22.15.0",
    "sharp": "^0.33.5",
    "uuid": "^8.3.2",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.15",
    "@types/mime-types": "^2.1.4",
    "@types/node": "22.7.9",
    "@types/opossum": "^8.1.8",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/pdfkit": "^0.12.12",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@types/yauzl": "^2.10.3",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
